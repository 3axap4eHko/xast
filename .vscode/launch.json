{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "JEST",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "node",
      "runtimeArgs": ["--nolazy"],
      "args": ["node_modules/.bin/jest", "src/__tests__/variable.ts"],
      "cwd": "${workspaceRoot}",
      "internalConsoleOptions": "openOnSessionStart",
      "skipFiles": ["<node_internals>/**", "node_modules/**"]
    },
  ]
}
